{
  "name": "obs-websocket-js",
  "version": "1.2.0",
  "description": "OBS Websocket API in Javascript, consumes @Palakis/obs-websocket",
  "author": {
    "name": "Brendan Hagan",
    "url": "haganbmj"
  },
  "license": "MIT",
  "repository": {
